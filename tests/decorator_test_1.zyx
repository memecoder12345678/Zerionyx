defun log_call(fn)
    defun wrapper(*vargs, **kargs)
        println("Calling function: " + slice(to_str(fn), 10, -1))
        result = fn(*vargs, **kargs)
        println("Function " + slice(to_str(fn), 10, -1) + " finished.")
        return result
    done
    return wrapper
done

&log_call
defun add(a, b) -> a + b

add(5, 3)

