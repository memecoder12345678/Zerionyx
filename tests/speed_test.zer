load "libs.time"
load "libs.sys"

N = 1000000
total = 0
i = 1
let start = time()

for i = 1 to N + 1 do
    total += i
done

end = time()

println("Sum: " + to_str(total))
println("Zerionyx ran in: " + to_str(end - start) + " seconds")
exit(0)

# Fastest: 1.63242268562317 seconds
# Slowest: 2.20334911346436 seconds
# Average: 1.91788589954376 seconds
# NOTE: all tests were run using PyPy
