load "libs.time"
load "libs.sys"

N = 1000000
Repeats = 10
times = []

for r = 1 to Repeats + 1 do
    total = 0
    start = time.time()
    for i = 1 to N + 1 do
        total += i
    done
    end = time.time()
    append(times, end - start)
done

let sum = 0
let fastest = (times>0)
let slowest = (times>0)

for t in times do
    sum += t
    if t < fastest do
        fastest = t
    done
    if t > slowest do
        slowest = t
    done
done

avg = sum / to_float(len(times), false)

println("Average: " + to_str(avg) + " s")
println("Fastest: " + to_str(fastest) + " s")
println("Slowest: " + to_str(slowest) + " s")
sys.exit(0)


