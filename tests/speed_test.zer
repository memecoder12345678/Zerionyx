load "libs.time"
load "libs.sys"

N = 1000000
total = 0
let start = time.time()

for i = 1 to N + 1 do
    total += i
done

end = time.time()

println("Sum: " + to_str(total))
println("Zerionyx ran in: " + to_str(end - start) + " seconds")
sys.exit(0)

# Fastest: 0.872158288955689 seconds
# Slowest: 0.915579557418823 seconds
# Average: 0,893868923187256 seconds
# NOTE: All tests were run using PyPy 3.11 (v7.3.19, 64-bit) on Intel Core i5-8250U.
