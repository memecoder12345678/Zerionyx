# libs.keyboard

defun keyboard_write(t)
    keyboard_write_fp(t)
    return none
done

defun press(k)
    keyboard_press_fp(k)
    return none
done

defun release(k)
    keyboard_release_fp(k)
    return none
done

defun keyboard_wait(k)
    keyboard_wait_fp(k)
    return none
done

defun is_pressed(k)
    return keyboard_is_pressed_fp(k)
done
