# libs.threading

defun thread_start(f, a)
    return thread_start_fp(f, a)
done

defun thread_sleep(s)
    thread_sleep_fp(s)
    return none
done

defun thread_join(h, t)
    thread_join(h, t)
    return none
done

defun thread_is_alive(t)
    return thread_is_alive_fp(t)
done

defun thread_cancel(t)
    thread_cancel_fp(t)
    return none
done
